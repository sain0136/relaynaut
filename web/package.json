{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,md,json}\"",
    "lint": "npx --yes oxlint src --config .oxlintrc.json --tsconfig ./tsconfig.json",
    "lint:fix": "npx --yes oxlint --fix src --config .oxlintrc.json --tsconfig ./tsconfig.json",
    "lint:ci": "npx --yes oxlint src --config .oxlintrc.json --tsconfig ./tsconfig.json --deny-warnings"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "oxlint": "^1.23.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}