{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,md,json}\"",
    "lint": "npx --yes oxlint src --config .oxlintrc.json --tsconfig ./tsconfig.json",
    "lint:fix": "npx --yes oxlint --fix src --config .oxlintrc.json --tsconfig ./tsconfig.json",
    "lint:ci": "npx --yes oxlint src --config .oxlintrc.json --tsconfig ./tsconfig.json --deny-warnings"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/vue-form": "^1.23.8",
    "@vueuse/core": "^14.0.0",
    "lucide": "^0.548.0",
    "primevue": "^4.4.1",
    "vue": "^3.5.22",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "oxlint": "^1.23.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "sass": "^1.93.2",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
